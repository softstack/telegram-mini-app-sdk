{
	"name": "@tconnect.io/evm-provider",
	"version": "0.0.6",
	"description": "",
	"author": "",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/softstack/telegram-mini-app-sdk.git"
	},
	"keywords": [
		"tezos"
	],
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"types": "dist/types/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/cjs/index.js",
			"import": "./dist/esm/index.js",
			"types": "./dist/types/index.d.ts",
			"default": "./dist/esm/index.js"
		}
	},
	"files": [
		"dist/**/*",
		"src/**/*"
	],
	"scripts": {
		"test": "echo \"No test specified\"",
		"prettier": "prettier --write \"./**/*.css\" \"./**/*.js\" \"./**/*.json\" \"./**/*.mjs\" \"./**/*.ts\" \"./**/*.tsx\" \"./**/*.html\" --ignore-path ../../.prettierignore --no-error-on-unmatched-pattern",
		"lint": "eslint ./src",
		"compile": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json",
		"build:clean": "rm -rf ./dist",
		"build": "npm run build:clean && npm run compile && rm ./dist/**/*.tsbuildinfo",
		"prepublishOnly": "npm run build && npm test && npm run lint"
	},
	"dependencies": {
		"@tconnect.io/core": "^0.0.4",
		"@tconnect.io/dapp-communication": "^0.0.4",
		"@tconnect.io/dapp-utils": "^0.0.5",
		"@tconnect.io/evm-api-types": "^0.0.6",
		"@twa-dev/sdk": "^7.10.1",
		"joi": "^17.13.3"
	}
}
